            "type": "cortex-debug",
            "servertype": "stutil",
            "device": "STM32F401RE",
            "interface": "swd",
            "preLaunchTask": "Build all",
            "svdFile": "",
            "swoConfig": {}
        },
        {
            "showDevDebugOutput": "parsed",
            "cwd": "${workspaceRoot}",
            "executable": "./build/Air_stats.elf",
            "name": "Debug STM32",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "preLaunchTask": "Build STM",
            "device": "",
            "configFiles": [
                "openocd.cfg"
            ]
        },
        {
            "showDevDebugOutput": "parsed",
            "cwd": "${workspaceRoot}",
            "executable": "./build/Air_stats.elf",
